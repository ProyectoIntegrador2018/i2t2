<div class="nested-fields">
  <div class="field">
    <%= f.label :name%>
    <%= f.text_field :name %>
    <%= f.label :date %>
    <%= f.date_select :date, discard_day: true, discard_month: true %>
    <%= f.check_box :_destroy, hidden: true %>
    <%= link_to_remove_association "remove award", f %>
  </div>
</div>
